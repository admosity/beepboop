include ../partials/sidebar.jade

div.dashboard-wrapper
  include ../partials/dashboard-header.jade
  div.dashboard-content
    div.buy-form
      div.buy-inner
        h1 buy 10 more keys
        p.info You can use a maximum of 10 keys at a time. Use this form to purchase another batch of 10 for only $2.00
        br
        h2 $2.00
        br
        form(ng-submit="")
          input(type="text" placeholder="shit")
          input(type="submit" class="btn btn-purple block" value="Buy")
          
    div.buy-form.thanks
      div.buy-inner
        h1 thank you!
        p.info Your payment has been received. You now have 10 more devices added to your BeepBoop account.
